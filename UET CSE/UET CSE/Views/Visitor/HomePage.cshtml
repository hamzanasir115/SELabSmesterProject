                    <p>Faculty of CSE Department,UET Lahore <a href="http://localhost:11751/Visitor/Faculty" class="genric-btn primary small" style="background-color:rgba(4,9,30,0.9)">Faculty</a>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="active-popular-carusel">
                @foreach (var item in ViewBag.Faculty)
                {
                    <div class="single-popular-carusel">
                        <div class="thumb-wrap relative">
                            <div class="thumb relative">
                                <div class="overlay overlay-bg"></div>
                                <img class="img-fluid" src="@Url.Content(@item.ImagePath)" alt="" style="width:263px;height:200px;">
                            </div>
                            <div class="meta d-flex justify-content-between">
                                <!--                @*  <p><span class="lnr lnr-users"></span>  <span @*class="lnr lnr-bubble"></span></p>*@
                                       -->
                                <br>
                            </div>
                        </div>
                        <div class="details">
                           
                            <h4>

                            @item.Name
                                <!--@Html.ActionLink((string)item.Name, "FacultyDetail", "Visitor", new { id = item.Id })
                            --></h4>
                            <p>
                                Designation : @item.Designation
                                <br />
                                Qualification : @item.Qualification
                                <br />
                            </p>
                        </div>
                    </div>
                }
                

            </div>
        </div>
</section>
<!-- End Faculty Section -->
