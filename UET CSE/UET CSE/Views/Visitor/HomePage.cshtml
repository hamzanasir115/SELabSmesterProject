<!-- Start upcoming-event Area -->
<section class="upcoming-event-area section-gap">
    <div class="container">
        <div class="row d-flex justify-content-center">
            <div class="menu-content pb-70 col-lg-8">
                <div class="title text-center">

                    <h1 class="mb-10"> <p id="Events"> Upcoming Events of UET CSE</h1></p>
                    <p>
                        Upcoming Events of the department of CSE, Uet Lahore <a href="http://localhost:11751/Visitor/Events" class="genric-btn primary small" style="background-color:rgba(4,9,30,0.9)">Events</a>
                    </p>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="active-upcoming-event-carusel">
                @{
                    foreach (var item in ViewBag.Events)
                    {
                        <div class="single-carusel row align-items-center">
                            <div class="col-12 col-md-6 thumb">
                                <img class="img-fluid" src="@Url.Content(@item.ImagePath)" style="width:263px;height:200px;">
                            </div>
                            <div class="detials col-12 col-md-6">
                                <p>@item.Start_Date - @item.End_Date</p>
                                <a href="http://localhost:11751/Visitor/EventsDetail">
                                    <h4>
                                        @item.Event_Name
                                    </h4>
                                </a>
                                <p>
                                    @item.Description
                                </p>
                            </div>
                        </div>
                    }
                }
                
                
</section>
<!-- End upcoming-event Area -->
<!-- Start Achievement Section-->
<section class="popular-course-area section-gap">
    <div class="container">
        <div class="row d-flex justify-content-center">
