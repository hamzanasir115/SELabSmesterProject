@model IEnumerable<UET_CSE.Models.AddDateSheet>

@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


@using (Html.BeginForm("ViewDatesheet", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <section class="event-form" style="background-color:aliceblue">
        <div class="container" style="background-color:aliceblue">
            <br><br><h2>@ViewBag.Title</h2><br><br>
            <form class="form-horizontal" action="/action_page.php">

                <table class="table">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Session)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Section)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Date)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Day)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Program)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Subject)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Supritendent_Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Time)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Hall)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Session)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Section)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Date)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Day)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Program)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Subject)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Supritendent_Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Time)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Hall)
                            </td>
                            <td>
                                @Html.ActionLink("Update", "UpdateDateSheet", new { id = item.Id }) |
                                @Html.ActionLink("Delete", "DeleteDateSheet", new { id = item.Id })
                            </td>
                        </tr>
                    }

                </table>
            </form>
        </div><br /><br /><br />
    </section>
}